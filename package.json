{
  "name": "context.reviews",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "npm run style:check && npm run lint",
    "dev": "nodemon ./dist/server/app.js --watch dist/server/",
    "build:prod": "npm run build-static:prod && npm run build-server:prod",
    "build-server:dev": "tsc --watch",
    "build-server:prod": "tsc",
    "lint": "npm run lint-server:prod && npm run lint-static:prod",
    "lint-server:prod": "eslint 'src/server/**/*.ts' && tsc --noEmit",
    "lint-static:dev": "svelte-check --watch",
    "lint-static:prod": "eslint 'src/static/**/*.{js,ts,svelte}' && svelte-check --ignore src/extension",
    "build-static:prod": "./static-prebuild.sh && vite build",
    "build-static:dev": "./static-prebuild.sh && vite",
    "build-extension:dev": "webpack --mode development --config-name extension --watch --env SITE_TARGET=dev",
    "build-extension:prod": "webpack --mode development --config-name extension --env SITE_TARGET=prod",
    "style:write": "prettier --write .",
    "style:check": "prettier --check . --loglevel warn",
    "extension:package": "npm run build-extension:prod && web-ext build --source-dir ./extension -o",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@prisma/client": "^3.2.1",
    "@types/page": "^1.11.5",
    "ajv": "^6.12.6",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.4",
    "cookie": "^0.4.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "~4.16.0",
    "express-session": "^1.17.1",
    "glob": "^7.1.6",
    "handlebars": "^4.7.7",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.0",
    "page": "^1.11.6",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.2.1",
    "prettier-plugin-svelte": "^2.5.0",
    "prom-client": "^12.0.0",
    "pug": "^3.0.2",
    "redis": "^3.1.2",
    "sass": "^1.32.5",
    "serialize-javascript": "^3.1.0",
    "sharp": "^0.25.4",
    "sheodox-ui": "^0.16.3",
    "svelte": "^3.38.2",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.9.8",
    "typescript": "^4.4.4",
    "uuid": "^8.3.1",
    "web-ext": "^6.4.0",
    "winston": "^3.3.3",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@types/connect-redis": "0.0.17",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/pg": "^7.14.4",
    "@types/redis": "^2.8.25",
    "@types/serialize-javascript": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.0",
    "nodemon": "^2.0.4",
    "prettier": "2.4.1",
    "prisma": "^3.2.1",
    "vite": "^2.6.14"
  }
}
