{
  "name": "context.reviews",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "npm run style:check && npm run lint",
    "dev": "nodemon ./dist/server/app.js --watch dist/server/",
    "build:prod": "npm run build-static:prod && npm run build-server:prod",
    "build-server:dev": "tsc --watch",
    "build-server:prod": "tsc",
    "lint": "npm run lint-server:prod && npm run lint-static:prod",
    "lint-server:prod": "eslint 'src/server/**/*.ts' && tsc --noEmit",
    "lint-static:dev": "svelte-check --ignore dist --watch",
    "lint-static:prod": "eslint 'src/{static,extension}/**/*.{js,ts,svelte}' && svelte-check --ignore dist",
    "build-static:prod": "./static-prebuild.sh && vite build",
    "build-static:dev": "./static-prebuild.sh && vite",
    "build-extension:dev": "webpack --mode development --config-name extension --watch --env SITE_TARGET=dev",
    "build-extension:prod": "webpack --mode development --config-name extension --env SITE_TARGET=prod",
    "style:write": "prettier --write .",
    "style:check": "prettier --check . --loglevel warn",
    "extension:package": "npm run build-extension:prod && npm run extension:package:firefox && npm run extension:package:chrome",
    "extension:package:firefox": "web-ext build --source-dir ./dist/extension-base -o -a dist/web-ext-artifacts/firefox",
    "extension:package:chrome": "web-ext build --source-dir ./dist/extension-chrome -o -a dist/web-ext-artifacts/chrome",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@prisma/client": "^3.2.1",
    "@types/page": "^1.11.5",
    "@types/webextension-polyfill": "^0.8.2",
    "ajv": "^6.12.6",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.4",
    "cookie": "^0.4.1",
    "cookie-parser": "~1.4.3",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "~4.18.2",
    "express-session": "^1.17.1",
    "glob": "^7.1.6",
    "handlebars": "^4.7.7",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^9.0.0",
    "page": "^1.11.6",
    "passport": "^0.6.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.2.1",
    "prettier-plugin-svelte": "^2.9.0",
    "prom-client": "^12.0.0",
    "pug": "^3.0.2",
    "redis": "^3.1.2",
    "serialize-javascript": "^6.0.1",
    "sharp": "^0.30.7",
    "sheodox-ui": "^0.18.26",
    "svelte-check": "^3.0.3",
    "svelte-loader": "^3.1.5",
    "svelte-preprocess": "^5.0.1",
    "uuid": "^8.3.1",
    "web-ext": "^7.1.0",
    "winston": "^3.3.3",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@types/connect-redis": "0.0.17",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^14.0.27",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/pg": "^7.14.4",
    "@types/redis": "^2.8.25",
    "@types/serialize-javascript": "^5.0.2",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.33.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^7.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.8.3",
    "prisma": "^3.2.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "svelte": "^3.55.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "webextension-polyfill": "^0.8.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2"
  }
}
